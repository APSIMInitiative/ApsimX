                        },
                        {
                          "Key": "Tt_EstablishToHarvest",
                          "Value": "1000"
                        },
                        {
                          "Key": "HarvestStage",
                          "Value": "Maturity"
                        },
                        {
                          "Key": "ExpectedYield",
                          "Value": "10"
                        },
                        {
                          "Key": "FieldLoss",
                          "Value": "0"
                        },
                        {
                          "Key": "ResidueRemoval",
                          "Value": "0"
                        },
                        {
                          "Key": "ResidueIncorporation",
                          "Value": "0"
                        },
                        {
                          "Key": "ResidueIncorporationDepth",
                          "Value": "0"
                        }
                      ],
