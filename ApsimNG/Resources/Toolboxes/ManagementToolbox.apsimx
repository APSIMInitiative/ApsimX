            },
            {
              "Key": "PatchCO2",
              "Value": "False"
            },
            {
              "Key": "PatchVP",
              "Value": "False"
            }
          ],
          "Name": "PatchWeather",
          "ResourceName": null,
          "Children": [],
          "Enabled": true,
          "ReadOnly": false
        }
