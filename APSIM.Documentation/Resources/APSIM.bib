@article{prentis_simultaneous_1996,
	title = {The simultaneous management of price and production risk.},
	abstract = {Price and yield variation are both important components of income risk in broadacre dryland farming. The development of a prototype model to facilitate the simultaneous management of these two kinds of risk is reported. The model uses data specific to three crops (sorghum, wheat and cotton) at four Australian locations (Emerald, Dalby, Narrabri and Gunnedah). Probability distributions of future yield for standard conditions were generated using the APSIM crop simulation models. A schedule of variable costs, of both production and marketing, is also included. Three price elements (futures, forward rate of foreign exchange and basis) are modelled separately. Combining the probability distributions of yield, variable costs and each component of price, and the correlations between them, using Monte Carlo simulation, provides estimates of the variation in gross margin associated with the purchase of different price instruments.},
	journal = {Proceedings of the 8th Australian Agronomy Conference, Toowoomba,   Queensland, Australia, 30 January-2 February, 1996.},
	author = {Prentis, D. and Holmes, P. and Cox, P.},
	year = {1996},
	note = {CABI:19981802813},
	pages = {454--457}
}

@article{probert_modelling_1995,
	title = {Modelling water, nitrogen, and crop yield for a long-term fallow management experiment},
	volume = {35},
	doi = {10.1071/EA9950941},
	abstract = {Two models that differ markedly in how they represent the crop-soil system have been used to simulate soil processes and crop production in the long-term experiment at Hermitage Research Station, Warwick, Queensland. The experiment was designed to examine the effects of tillage, stubble management, and nitrogen (N) fertiliser on the productivity of a winter cereal-summer fallow cropping system. It commenced in 1968 and the treatments have been maintained until the present. CENTURY operates on a monthly time step, considers all soil N transformations to occur in a single soil layer, and has a very simple crop growth routine that does not deal with crop phenology. APSIM provides a framework whereby a model of a cropping system is configured from component modules, which operate on a daily time step. For simulating the Hermitage experiment, modules to represent the dynamics of soil-water, N, surface residues, and growth of a wheat crop were used. The water and N modules deal with a multilayered soil, whilst the wheat module develops leaf area, intercepts light, and accumulates and partitions dry matter in response to weather, soil-water, and N. Both models were specified to simulate the whole experimental period (1969-92) as a continuous run. The ability of these models to simulate grain yields, soil-water and drainage, nitrate-N, and soil organic matter were examined. Both models predict, in agreement with the observed data, that for this continuous cereal cropping system there has been a decline in soil organic matter for all the treatments and a reduction through time in the capacity of the soil to mineralise and accumulate nitrate during the fallows. CENTURY performed better than APSIM in predicting the relative yields of the N treatments but was less satisfactory than APSIM for absolute grain yield, soil-water, and drainage. Yield predictions with APSIM were sensitive to carry-over errors in the water balance from one season to the next, so that in some seasons large errors occurred in the predicted relative yields. Both models reproduced the observations well enough to indicate their suitability for providing useful insights into the behaviour of cropping systems where the focus is on depletion of soil fertility.},
	number = {7},
	journal = {Australian Journal of Experimental Agriculture},
	author = {Probert, M. E. and Keating, B. A. and Thompson, J. P. and Parton, W. J.},
	year = {1995},
	note = {WOS:A1995TU75200014},
	pages = {941--950}
}

@article{anonymous_taking_1994,
	title = {Taking erosion into account.},
	abstract = {The cropping systems simulation models PERFECT (Productivity, Erosion, Run-off Functions to Evaluate Conservation Techniques) and APSIM are being used to analyse the effect of soil erosion on long-term yields under different conditions in Queensland and New South Wales, Australia. Three cropping strategies were tested: continuous wheat cropping, a wheat-sorghum rotation, and opportunity cropping using wheat, sorghum and sunflowers. The simulation models were used to examine how crop water use matched available water and to provide information for planning future cropping programmes. The profitabilities of the three system were compared. The impact of soil erosion on long-term yields under a continuous wheat cropping system was also simulated. Annual soil erosion was predicted on different slopes and at different soil depths under different management options (Burnt stubble, disc/chisel, stubble-mulched, zero tillage). A change from zero tillage to stubble-burnt resulted in a 20-fold increase in predicted erosion.},
	number = {163},
	journal = {Rural Research},
	author = {Anonymous},
	year = {1994},
	note = {CABI:19941908581},
	pages = {12--13}
}

