@article{lorenconi_calibration_2010-1,
	title = {Calibration of the {ORYZA}-{APSIM} model for the upland rice in {Brazil}.},
	volume = {85},
	abstract = {With the purpose of calibrating and evaluating the ORYZA-APSIM model for the upland rice in Brazil, 17 field experiments were carried out at EMBRAPA, in four upland rice Brazilian regions (Santo Antonio de Goias-GO, Sorriso-MT, Gurupi-TO and Teresina-PI), from 2005/2006 to 2008/2009 seasons. The model was calibrated and evaluated for an upland rice, cultivar BRS-Primavera. The following variables were defined in the calibration: (i) daily effective heat units, (ii) phenological development rates at each crop stage, (iii) fractions of the dry matter of leaves, stems and storage organs during the developmental crop stages, (iv) initial growth rate of leaf area and (v) parameters used to define the specific leaf area. The model did not present satisfactory performance to simulate the duration of different phenological phases of development for the Teresina-PI region (the numbers of simulated days were, in average, eight days higher than the observed values).},
	number = {3},
	journal = {Revista de Agricultura (Piracicaba)},
