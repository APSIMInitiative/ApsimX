                        "",
                        "        //User inputs from properties tab",
                        "        [Separator(\"Generic irrigation management\")]",
                        "        [Description(\" Enable automatic irrigation management?              \")]",
                        "        public bool IrrigationIsEnabled {get; set;}",
                        "",
