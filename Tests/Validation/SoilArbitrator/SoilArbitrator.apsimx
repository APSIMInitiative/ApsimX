                            "            if (iniDate.DayOfYear < endDate.DayOfYear)",
                            "            {",
                            "                // period is within one year, ex: summer in the northern hemisphere",
                            "                if ((theDay.DayOfYear >= iniDate.DayOfYear) && (theDay.DayOfYear <= endDate.DayOfYear))",
                            "                    result = true;",
                            "            }",
                            "            else",
