                {
                  "$type": "Models.Weather, Models",
                  "FileName": "%root%\\Examples\\WeatherFiles\\lincoln.met",
                  "ExcelWorkSheetName": null,
                  "Name": "Weather",
                  "Children": [],
                  "IncludeInDocumentation": true,
