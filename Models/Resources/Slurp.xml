 <Plant>
	<Name>Slurp</Name>
	<CropType>Potato</CropType>
	<Cultivar>
		<Name>StaticCrop</Name>
		<Alias>StaticCrop</Alias>
	</Cultivar>
	<Arbitrator>
		<Name>Arbitrator</Name>
		<NArbitrationOption>PriorityThenRelativeAllocation</NArbitrationOption>
		<DMArbitrationOption>RelativeAllocation</DMArbitrationOption>
	</Arbitrator>
	<Root>
		<Name>Root</Name>
		<CompositeBiomass>
			<Name>Live</Name>
			<Propertys>
				<string>[Root].LayerLive</string>
			</Propertys>
		</CompositeBiomass>
		<CompositeBiomass>
			<Name>Dead</Name>
			<Propertys>
				<string>[Root].LayerDead</string>
			</Propertys>
		</CompositeBiomass>
		<Constant>
			<Name>NitrogenDemandSwitch</Name>
			<Value>1</Value>
		</Constant>
		<AirTemperatureFunction>
			<Name>TemperatureEffect</Name>
			<XYPairs>
				<Name>XYPairs</Name>
				<X>
					<double>0</double>
					<double>26</double>
					<double>35</double>
				</X>
				<Y>
					<double>1</double>
					<double>1</double>
					<double>1</double>
				</Y>
			</XYPairs>
		</AirTemperatureFunction>
		<Constant>
			<Name>RootFrontVelocity</Name>
			<Value>0</Value>
		</Constant>
		<Constant>
			<Name>PartitionFraction</Name>
			<Value>0</Value>
		</Constant>
		<Constant>
			<Name>MaximumNConc</Name>
			<Value>0.01</Value>
		</Constant>
		<Constant>
			<Name>MaxDailyNUptake</Name>
			<Value>6</Value>
		</Constant>
		<Constant>
			<Name>MinimumNConc</Name>
			<Value>0.01</Value>
		</Constant>
		<Constant>
			<Name>KLModifier</Name>
			<Value>1</Value>
		</Constant>
		<InitialDM>0</InitialDM>
		<SpecificRootLength>40000</SpecificRootLength>
		<KNO3>0.02</KNO3>
		<KNH4>0</KNH4>
	</Root>
	<SimpleLeaf>
		<Name>Leaf</Name> 
		<Biomass>
			<Name>Live</Name>
		</Biomass>
		<Biomass>
			<Name>Dead</Name>
		</Biomass>
		<Constant>
			<Name>FTFunction</Name>
			<Value>1</Value>
		</Constant>	  
		<Constant>
			<Name>FVPDFunction</Name>
			<Value>1</Value>
		</Constant>
		<Constant>
			<Name>PotentialBiomass</Name>
			<Value>5</Value>
		</Constant>
		<PartitionFractionDemandFunction>
			<Name>DMDemandFunction</Name>
			<Constant>
				<Name>PartitionFraction</Name>
				<Value>1</Value>
			</Constant>
		</PartitionFractionDemandFunction>
		<Constant>
			<Name>NConc</Name>
			<Value>.05</Value>
		</Constant>
		<Constant>
			<Name>LAIFunction</Name>
			<Value>5</Value>
		</Constant>
		<Constant>
			<Name>ExtinctionCoefficientFunction</Name>
			<Value>0.7</Value>
		</Constant>
		<Constant>
			<Name>HeightFunction</Name>
			<Value>150</Value>
		</Constant>

	</SimpleLeaf>
</Plant>
