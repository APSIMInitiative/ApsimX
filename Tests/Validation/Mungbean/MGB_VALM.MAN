
 [standard.manager.init]       

     dateset = 0
     emer_date = 0
     endjuv_date = 0
     anth_date = 0
     mat_date = 0

 [standard.manager.end_of_day]

     if mungbean.stage = 3 and dateset = 0 then
        emer_date = das
        dateset = 1
     endif

     if mungbean.stage = 4 and dateset = 1 then
        endjuv_date = das
        dateset = 2
     endif

     if mungbean.stage = 6 and dateset = 2 then
        anth_date = das
        dateset = 3
     endif
      
     if mungbean.stage = 9 and dateset = 3 then
        mat_date = das
        dateset = 4
     endif

if (mungbean.stage_name = 'maturity' or mungbean.plant_status = 'dead') then

   report do_output
   mungbean harvest 
   mungbean kill_crop 
   mungbean end_crop
   dateset = 0

endif

