                                "        public enum removeDMTypes",
                                "        {",
                                "            /// <summary>Remove all DM</summary>",
                                "            RemoveAll,",
                                "            /// <summary>Remove no DM</summary>",
                                "            RemoveNone,",
                                "            /// <summary>Remove a fraction based on digestibility</summary>",
                                "            BasedOnDigestibility",
                                "        }",
