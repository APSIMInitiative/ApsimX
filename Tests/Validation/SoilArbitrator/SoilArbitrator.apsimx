                                {
                                  "Key": "AllowFertiliser",
                                  "Value": "yes"
                                },
                                {
                                  "Key": "Depth2SoilN",
                                  "Value": "600"
                                },
                                {
                                  "Key": "CriticalSoilN",
                                  "Value": "100"
                                },
                                {
                                  "Key": "FertNAmount",
                                  "Value": "50"
                                },
                                {
                                  "Key": "FertType",
                                  "Value": "UreaN"
                                },
                                {
                                  "Key": "FertApplicationDepth",
                                  "Value": "1"
                                },
                                {
                                  "Key": "FertMinimumDays",
                                  "Value": "1"
                                }
                              ],
                              "Name": "AutomaticFertiliser",
