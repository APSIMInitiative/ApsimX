                          "Code": "using Models.Climate;\nusing APSIM.Shared.Utilities;\nusing Models.Soils.Nutrients;\nusing Models.Soils;\nusing Models.PMF;\nusing Models.Core;\nusing System.Xml.Serialization;\nusing System;\nusing Models.AgPasture;\nusing Models.Interfaces;\nusing System.Diagnostics;\n\nnamespace Models\n{\n    [Serializable] \n    [System.Xml.Serialization.XmlInclude(typeof(Model))]\n    public class Script : Model\n    {\n    \n    \t//<Link()> Dim MyPaddock As Paddock\n   \t\t[Link] Swim3 swim;\n   \t\t[Link] Nutrient nutrient;\n   \t\t[Link] Clock clock;\n   \t\t[Link] ISoilWater waterBalance;\n\n   \t\t// Parameter variables:\n   \t\t[Description(\"Date that autumn urine is applied\")]\n   \t\tpublic string AutumnUrinePatch_date_str { get; set;}\n   \t\t\n   \t\t[Description(\"Date that spring urine is applied\")]\n   \t\tpublic string SpringUrinePatch_date_str { get; set;}\n\n  \t\t[Description(\"The amount of urine as a depth of water (mm):\")]\n   \t\t[Units(\"mm\")]\n  \t\tpublic double Urine_Water_to_add { get; set;}\n  \t\t\n   \t\t[Description(\"Amount of N in the urine (kg/ha):\")]\n   \t\t[Units(\"kgN/ha\")]\n   \t\tpublic double Urine_N_to_add { get; set;}\n   \t\t\n  \t\t//[Description(\"Type the amount of Br to apply (kg/ha):\")]\n   \t\t//[Units(\"kg/ha\")] \n   \t\t//public double Br_to_add { get; set;}\n   \n  \t\t[Description(\"The depth reached by urine (mm):\")]\n   \t\tpublic double Depth_for_urine { get; set;}\n   \n   \t\tpublic enum UrineLevelOptions { None, Autumn, Spring }\n   \t\t\n   \t\t// Variables read from other modules\n\t    [Description(\"The urine level:\")]\n   \t\tpublic UrineLevelOptions UrineLevel {get; set; }    \n\n\t\t// Output variables\n\t\tprivate double Urine;        // Amount of urine applied today\n   \t\tprivate DateTime UrinePatch_date;\n\n   \t\t// Internal variables\n   \t\tprivate DateTime AutumnUrinePatch_date;\n   \t\tprivate DateTime SpringUrinePatch_date;\n   \t\tprivate DateTime AppDate;             // The date when urine applications start (in date format)\n   \t\tprivate DateTime Today;               // The date of today (in date format)\n   \t\tprivate double[] Urine_prop;          // The potential proportion of urine for each soil layer\n   \t\tprivate double[] Urine_prop_cum;      // The potential proportion of urine for each soil layer\n   \t\tprivate double SoilDepth;             // The soil depth (mm)\n   \t\tprivate bool Add_Urine;\n\n\t\t[EventSubscribe(\"StartOfSimulation\")]\n        private void OnStartOfSimulation(object sender, EventArgs e)\n\t\t{\n\t\t\t//Debugger.Break();\n\t\t\t\n      \t\t// Put the dates into date format:\n      \t\tAutumnUrinePatch_date = DateTime.Parse(AutumnUrinePatch_date_str);\n      \t\tSpringUrinePatch_date = DateTime.Parse(SpringUrinePatch_date_str);\n      \t\n\t      \tif (UrineLevel == UrineLevelOptions.None)\n\t         \tUrinePatch_date = new DateTime(3000, 12, 31);    // ie - not within this simulation\n      \t\telse if (UrineLevel == UrineLevelOptions.Autumn)\n         \t\tUrinePatch_date = AutumnUrinePatch_date; \n      \t\telse if (UrineLevel == UrineLevelOptions.Spring)\n         \t\tUrinePatch_date = SpringUrinePatch_date; \n      \t\telse\n         \t\tthrow new Exception(\"Incorrect urine date information\");\n\n      \t\tUrine_prop = new double[waterBalance.Thickness.Length];\n  \t\t    Urine_prop_cum = new double[waterBalance.Thickness.Length];\n  \n      \t\tSoilDepth = 0.0;\n      \t\tfor (int z = 0; z < waterBalance.Thickness.Length; z++)\n      \t\t{\n\t        \tif (SoilDepth <= Depth_for_urine)   // this is at start depth\n           \t\t\tUrine_prop[z] = waterBalance.Thickness[z] / Depth_for_urine;\n         \t\telse\n            \t\tUrine_prop[z] = 0.0;\n         \n         \t\tSoilDepth += waterBalance.Thickness[z];   // no updated to end current depth\n         \n         \t\tif (z == 0)\n            \t\tUrine_prop_cum[z] = Urine_prop[z];\n         \t\telse\n            \t\tUrine_prop_cum[z] = Urine_prop[z] + Urine_prop_cum[z - 1];\n\n         \t\tif (Urine_prop_cum[z] <= 1.0)\n         \t\t{\n            \t\t// do nothing\n            \t}\n         \t\telse\n         \t\t{\n            \t\tUrine_prop[z] = Math.Max(0.0, 1.0 - Urine_prop_cum[z - 1]);\n            \t\tUrine_prop_cum[z] = Urine_prop[z] + Urine_prop_cum[z - 1];\n         \t\t}\n      \t\t}\n    \t}\n    \t\n\t\t[EventSubscribe(\"DoManagement\")]\n        private void OnDoManagement(object sender, EventArgs e)\n      \t{\n      \t\tUrine = 0.0;\n   \n\t      \tAdd_Urine = clock.Today == UrinePatch_date;\n      \n      \t\tif (Add_Urine)\n      \t\t{\n         \t\tdouble[] Ini_swdep = waterBalance.SWmm;\n         \t\tdouble[] Ini_urea  = nutrient.Urea.kgha;\n\n\t\t        Urine = Urine_N_to_add;\n\n         \t\tdouble[] New_swdep = new double[waterBalance.Thickness.Length];         \n         \t\tdouble[] New_urea = new double[waterBalance.Thickness.Length];\n\n\t\t        for (int z = 0; z < waterBalance.Thickness.Length; z++)\n\t\t        {\n            \t\tNew_swdep[z] = Ini_swdep[z] + Urine_prop[z] * Urine_Water_to_add;\n            \t\tNew_urea[z] = Ini_urea[z] + Urine_prop[z] * Urine_N_to_add;\n            \t}\n\t         \n\t\t        nutrient.Urea.SetKgHa(SoluteSetterType.Other, New_urea);\n      \t\t}\n      \t}\n\t}\n}    ",
