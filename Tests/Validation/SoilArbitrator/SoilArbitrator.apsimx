                                },
                                {
                                  "Key": "RemoveAmountType",
                                  "Value": "ResidualDM"
                                },
                                {
                                  "Key": "AmountGiven",
                                  "Value": "2250"
                                },
                                {
                                  "Key": "DMRemoveType",
                                  "Value": "RemoveAll"
                                },
                                {
                                  "Key": "FractionN2Remove",
                                  "Value": "1"
                                },
                                {
                                  "Key": "ProportionN2Dung",
                                  "Value": "0"
                                }
                              ],
                              "Name": "CutRotation",
                              "ResourceName": null,
                              "Children": [],
                              "Enabled": true,
                              "ReadOnly": false
                            }
