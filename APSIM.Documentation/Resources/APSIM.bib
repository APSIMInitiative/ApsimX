@article{whish_integrating_2015-1,
	title = {Integrating pest population models with biophysical crop models to better represent the farming system},
	volume = {72},
	issn = {1364-8152},
	url = {http://www.sciencedirect.com/science/article/pii/S1364815214002990},
	doi = {10.1016/j.envsoft.2014.10.010},
	abstract = {Farming systems frameworks such as the Agricultural Production Systems simulator (APSIM) represent fluxes through the soil, plant and atmosphere of the system well, but do not generally consider the biotic constraints that function within the system. We designed a method that allowed population models built in DYMEX to interact with APSIM. The simulator engine component of the DYMEX population-modelling platform was wrapped within an APSIM module allowing it to get and set variable values in other APSIM models running in the simulation. A rust model developed in DYMEX is used to demonstrate how the developing rust population reduces the crop's green leaf area. The success of the linking process is seen in the interaction of the two models and how changes in rust population on the crop's leaves feedback to the APSIM crop modifying the growth and development of the crop's leaf area. This linking of population models to simulate pest populations and biophysical models to simulate crop growth and development increases the complexity of the simulation, but provides a tool to investigate biotic constraints within farming systems and further moves APSIM towards being an agro-ecological framework.},
	urldate = {2016-12-08},
	journal = {Environmental Modelling \& Software},
	author = {Whish, Jeremy P. M. and Herrmann, Neville I. and White, Neil A. and Moore, Andrew D. and Kriticos, Darren J.},
	month = oct,
	year = {2015},
	keywords = {Agro-ecological modelling, APSIM, Biotic constraints, DYMEX, Farming systems modelling, Modelling, Puccinia striiformis, Stripe rust},
	pages = {418--425},
	file = {ScienceDirect Full Text PDF:C\:\\Work\\Zotero\\Profiles\\sjdp2frt.default\\zotero\\storage\\Z428IDTK\\Whish et al. - 2015 - Integrating pest population models with biophysica.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Work\\Zotero\\Profiles\\sjdp2frt.default\\zotero\\storage\\APJ2R5RI\\S1364815214002990.html:text/html}
