                              "Key": "seasonEnd",
                              "Value": "30-May"
                            },
                            {
                              "Key": "seasonsAllocation",
                              "Value": "10000"
                            },
                            {
                              "Key": "triggerDeficit",
                              "Value": "99"
                            },
                            {
                              "Key": "targetDeficit",
                              "Value": "100"
                            },
                            {
                              "Key": "returndays",
                              "Value": "1"
                            },
                            {
                              "Key": "maximumAmount",
                              "Value": "5"
                            },
                            {
                              "Key": "depthPAWC",
                              "Value": "600"
                            }
                          ],
                          "Name": "AutomaticIrrigation",
                          "IncludeInDocumentation": true,
                          "Enabled": true,
                          "ReadOnly": false
                        }
