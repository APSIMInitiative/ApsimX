@article{nelson_long-term_1997,
	title = {Long-term effects of land management on soil erosion, crop yield and on-farm economics in the {Philippines}.},
	abstract = {This chapter of the report on the ACIAR Project PN 9201 introduces results from a special project which links ACIAR Projects PN 9201 and 9211. This special project uses cropping systems simulation models (the Agricultural Production Systems Simulator -APSIM) and an economic analysis to help predict the long term effects of land management on soil properties. Also provided in this chapter is a report on the outcome of the project which focuses on the comparison of traditional farming practices with a particular agronomic soil conservation management system in which the crop of interest is intercropped between leguminous hedgerows planted on the contour to form an alley-cropping system.},
	number = {40},
	journal = {ACIAR Technical Reports Series},
	author = {Nelson, R. A. and Dimes, J. D. and Silburn, D. M. and Paningbatan, E. P. and Cramb, R. A. and Mamicpic, M. A.},
	editor = {Coughlan, K. J. and Rose, C. W.},
	year = {1997},
	note = {CABI:19981906489},
	pages = {111--138}
}

@book{oosterom_applications_1997,
	title = {Applications for cropping systems simulation models in on-farm nutrient management research.},
	abstract = {The Integrated Systems Project 1 (ISP1) of ICRISAT cropping systems modelling initiative is described in which the Agricultural Production Systems Simulator (APSIM) software package takes a central place. APSIM is structured around a central 'engine', via which modules (for crop growth, soil water, soil nutrients, etc.) communicate. By linking multiple modules to this engine, it is possible to simulate crop rotations and crop mixtures. However, several of the modules that are relevant to the cropping systems of western Rajasthan, India, that are based on pearl millet ( Pennisetum glaucum), are not yet available in APSIM and need to be developed and validated. APSIM can then be used for applications of three types: (1) environmental characterization, (2) matching crop growth and development with resource availability, and (3) farm management options. On-farm nutrient management research can play an important role in obtaining the necessary data sets for validating modules of a cropping systems model. At a later stage, simulation modelling can help to set the agenda for future on-station and on-farm nutrient management research.},
	author = {Oosterom, E. J. van},
	editor = {Seeling, B. and Joshi, N. L.},
	year = {1997},
	note = {CABI:19981902720}
}

@article{hammer_modelling_1996,
	title = {Modelling sorghum in {Australia}: the state of the science and its role in the pursuit of improved practices.},
	shorttitle = {Modelling sorghum in {Australia}},
	abstract = {It is argued that effective crop modelling must combine `scientific' modelling to enhance understanding and generality with `engineering' modelling to retain a focus on prediction and problem-solving. Developments in crop modelling of sorghum, interaction with research to improve scientific understanding, and the interaction with decision-making to pursue improved practices are considered, with the focus on activities relevant to sorghum in Australia. The Agricultural Production System Simulator (APSIM) has been developed to simulate crop production, soil fertility, soil erosion and drainage from dryland grain production systems in Australia. APSIM includes a generic crop template, which incorporates subroutines for crop growth and development. The interaction of modelling with research has progressed in two key areas. Firstly, crop physiology field research activities have been designed around perceived weaknesses in the modelling capability, particularly water-by-nitrogen interactions, allocation of carbohydrate to grain, and dynamics of tillering. Crop-soil-climate databases also have an important role in the co-ordination of this work. Secondly, there has been interaction of modelling with genotypic improvement research. Specific examples of the use of sorghum modelling for assessing hypotheses about trait action and value are presented. Modelling has also improved decision-making by evaluating potential crop management decision options. Relevant and effective communication processes and participation of farmers/advisers are essential components in this interaction. The effect of time of planting on sorghum is used to explore these issues as simulation analyses and the way in which farmer/adviser perceptions differed. A flexible look-up database, `Whopper Cropper', is presented as a means to provide access to simulation analyses of decision options in a form most relevant to farmers/advisers. An example showing the potential use of seasonal forecasting in nitrogen fertilizer decisions is presented and discussed.},
	number = {93},
	journal = {AIAS Occasional Publication},
	author = {Hammer, G. L. and Chapman, S. C. and Muchow, R. C.},
	year = {1996},
	note = {CABI:19970700848},
	pages = {43--61}
}

@article{jones_nitrogen_1996,
	title = {Nitrogen inputs from a pasture legume in rotations with cereals in the semi-arid tropics of northern {Australia}: {Experimentation} and modelling on a clay loam soil},
	volume = {36},
	shorttitle = {Nitrogen inputs from a pasture legume in rotations with cereals in the semi-arid tropics of northern {Australia}},
	doi = {10.1071/EA9960985},
	abstract = {Two experiments on a Tippera clay loam soil (alfisol) at Katherine, Northern Territory, investigated the nitrogen (N) benefit from legume pasture leys of Caribbean style (Stylosanthes hamata cv. Verano) to subsequent maize crops. Nitrogen uptake and yield of the maize crops were higher after the Verano leys than after a grass ley, the effect persisting into the second crop. The 1-year Verano ley was estimated to have increased N uptake by the maize crops by about 30 kg/ha, and the 3-year ley by about 55 kg/ha over that for the grass. Removing some of the Verano dry matter (DM) as hay at the end of each growing season reduced the subsequent benefit compared with treatments where the Verano},
	number = {8},
	journal = {Australian Journal of Experimental Agriculture},
	author = {Jones, R. K. and Probert, M. E. and Dalgliesh, N. P. and McCown, R. L.},
	year = {1996},
	note = {WOS:A1996WL12300008},
	pages = {985--994}
}

@article{verburg_methodology_1996,
	title = {Methodology in soil water and solute balance modelling: an evaluation of the {APSIM}-{Soil} {Wat} and {SWIMv}2 models. {Report} of an {APSRU}/{CSIRO} {Division} of {Soils} workshop held in {Brisbane}, {Australia}, 16-18 {May} 1995.},
	shorttitle = {Methodology in soil water and solute balance modelling},
	abstract = {APSIM (Agricultural Production Systems SIMulator) developed for the dynamic simulation of agricultural systems, has a modular structure which facilitates a plug-in-pull-out approach for the integration of various simulation models. This report describes the simulation results obtained using APSIM-Soil Wat and SWIMv2 (Soil Water Infiltration and Movement) for 2 different experiments, and various applications of simulation modelling. Details of the model structure are discussed and descriptions of the experimental data and simulations for 2 of the data sets are presented. The conclusions from a workshop held in 1995 in Brisbane, Australia, in which water balance models were compared with data sets representing different systems are discussed.},
	number = {131},
	journal = {Divisional Report - CSIRO Division of Soils},
