}

@article{verburg_swimv2.1._1996,
	title = {{SWIMv}2.1. {User} manual.},
	abstract = {SWIM (Soil Water Infiltration and Movement) is a software package developed within CSIRO Division of Soils for simulating infiltration, evapotranspiration and redistribution. The first part of this manual covers the theoretical background of the SWIMv2.1 features, the second part gives precise instructions for preparation of the input files and running of the model, and gives some examples illustrating various aspects of the model input in output. The third part consists of 4 appendices including a list of publications related to SWIMv1, SWIMv2 and APSIM (Agricultural Production Systems SIMulator)-SWIM.},
