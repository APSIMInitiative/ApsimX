            <Code><![CDATA[// Auto fertilise per Lee model in Apsim classic

using System;
using Models.Core;
using Models.PMF;
using Models.Soils;
using APSIM.Shared.Utilities;
namespace Models
{
	[Serializable]
	public class Script : Model
	{
		[Link] Clock Clock;
		[Link] Fertiliser fertiliser;
		[Link] ISummary summary = null;
		[Link] Soil soil;

		private DateTime lastApplicationDate;

		[Description("Maximum Amount of NO3 required in top 3 layers (kg/ha)")]
		public double fertCriteria { get; set; }

		[Description("Critical Amount of NO3 in top 3 layers (kg/ha)")]
		public double fertCritical { get; set; }

		[Description("Minimum period between applications (days)")]
		public double fertMinDays { get; set; }

		
		[EventSubscribe("DoManagement")]
		private void OnDoManagement(object sender, EventArgs e)
		{						
			double NO3_available = soil.SoilNitrogen.NO3[0] + soil.SoilNitrogen.NO3[1] + soil.SoilNitrogen.NO3[2];
						
			double daysSinceLastApplication = (Clock.Today - lastApplicationDate).TotalDays;
			
			if (daysSinceLastApplication >= fertMinDays)
			{			
				if (NO3_available <= fertCritical)
				{
					double nDeficit = fertCriteria - NO3_available;				
					fertiliser.Apply(nDeficit, Fertiliser.Types.UreaN, 50);	// 50mm per LEE
					lastApplicationDate = Clock.Today;
					//summary.WriteMessage(this, "Applied " + nDeficit + " (was " + NO3_available.ToString() + ")");
					
				}
			}
		}
	}
}
]]></Code>
