          "CodeArray": [
            "using System.Linq;",
            "using System;",
            "using Models.Core;",
            "using Models.PMF;",
            "using System.Collections.Generic;",
            "using Models.Soils;",
            "using APSIM.Shared.Utilities;",
            "using Models.Interfaces;",
            "",
            "namespace Models",
            "{",
            "    [Serializable]",
            "    public class Script : Model",
            "    {",
            "        [Link] ISummary summary = null;",
            "        private Zone[] Zones {get; set; }",
            "        ",
            "        private double[] SoilWaterDeficit { get; set; }",
            "                ",
            "        private double DaysSinceIrrig = 0;",
            "        ",
            "        [EventSubscribe(\"StartOfSimulation\")]",
            "        private void DoStartOfSimulation(object sender, EventArgs e)",
            "        {",
            "               //Set up our list on zones in the simulation",
            "               List<Zone> zones = new List<Zone>();",
            "               foreach (Zone zone in this.Parent.FindAllInScope<Zone>().OfType<IModel>().ToList())",
            "               {",
            "                   zones.Add(zone as Zone);",
            "                   summary.WriteMessage(this, \"zone: \" + zone.Name, MessageType.Diagnostic);",
            "               }",
            "               Zones = zones.ToArray();",
            "               ",
            "               //Set up local solution variables",
            "               SoilWaterDeficit = new double[Zones.Length];",
            "        }",
            "        ",
            "        [EventSubscribe(\"DoManagement\")]",
            "        private void DoManagement(object sender, EventArgs e)",
            "        {",
            "            DaysSinceIrrig += 1;",
            "            //Calculate soil water deficit in each zone",
            "            if (DaysSinceIrrig > 10)",
            "            {",
            "              for (int i = 0; i < Zones.Length; i++)",
            "              {",
            "              var waterBalance = Zones[i].FindInScope<ISoilWater>();",
            "              var soilPhysical = Zones[i].FindInScope<IPhysical>();",
            "              SoilWaterDeficit[i] = MathUtilities.Sum(soilPhysical.DULmm) - MathUtilities.Sum(waterBalance.SWmm);",
            "              Irrigation irrigation = Zones[i].FindInScope<Irrigation>() as Irrigation;",
            "              irrigation.Apply(SoilWaterDeficit[i]);",
            "              }",
            "              DaysSinceIrrig = 0;",
            "            }",
            "        }",
            "        ",
            "    }",
            "}"
          ],
