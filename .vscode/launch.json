{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [

        {
            "name": "ApsimNG: Debug",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "Build Debug",
            "program": "${workspaceFolder}/bin/Debug/net8.0/ApsimNG.dll",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "requireExactSource": false,
            "logging": {
                "moduleLoad": false,
                "threadExit": false
            },
            "symbolOptions": {
                "searchMicrosoftSymbolServer": false
            }
        },
        {
            "name": "ApsimNG: Release",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "Build Release",
            "program": "${workspaceFolder}/bin/Release/net8.0/ApsimNG.dll",
            "cwd": "${workspaceFolder}",
            "requireExactSource": true,
            "symbolOptions": {
                "searchMicrosoftSymbolServer": false
            }
        },
        {
            "name": "APSIM.Cli: Debug",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "Clean Debug",
            "program": "${workspaceFolder}/bin/Debug/net8.0/apsim.dll",
            "args": [
                "document",
                "${workspaceFolder}/Examples/Wheat.apsimx"
            ],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Models: Debug",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "Clean Debug",
            "program": "${workspaceFolder}/bin/Debug/net8.0/Models.exe",
            "args": [
                "C:\\Work\\Repos\\APSIMInitiative\\ApsimX\\Examples\\Stock.apsimx"
            ],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "APSIM Server: Debug",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "Clean Debug",
            "program": "${workspaceFolder}/bin/Debug/net8.0/ApsimZMQServer.dll",
            "args": [
                "-v", "-a", "127.0.0.1",
                "-p", "57220", "-P", "interactive",
                "-f", "${workspaceFolder}/Tests/Simulation/ZMQ-Sync/ZMQ-sync.apsimx"
            ],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        },
        {
            "name": "Autodocs",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "Clean Debug",
            "program": "${workspaceFolder}/bin/Debug/net8.0/APSIM.Documentation.dll",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false,
        },
        {
            "name": "Apsim.Workflow: Debug",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/bin/Debug/net8.0/Apsim.Workflow.dll",
            "preLaunchTask": "Build Debug",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "requireExactSource": false,
            "logging": {
                "moduleLoad": false,
                "threadExit": false
            },
            "args": [
                "-d",
                "C:/Users/RIC394/source/repos/ApsimX/Tests/Validation/Canola",
                // "C:/Users/RIC394/source/repos/ApsimX/Tests/Validation/Maize",
                "-v",
                // "-l"
            ]
        },
        {
            "name": "Apsim.Workflow: Splitter",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/bin/Debug/net8.0/Apsim.Workflow.dll",
            "preLaunchTask": "Build Debug",            
            "cwd": "${workspaceFolder}",            
            "console": "integratedTerminal",
            "justMyCode": false,
            "requireExactSource": false,
            "logging": {
                "moduleLoad": false,
                "threadExit": false
            },
            "symbolOptions": {
                "searchMicrosoftSymbolServer": false
            },
            "args": [
                "-d",
                "C:/git/ApsimX/Tests/Validation/Wheat/Wheat.apsimx",
                "-s",
                "C:/git/ApsimX/Tests/Validation/Wheat/split.json"
            ]
        },
        {
            "name": "Apsim.Workflow: Sim Count",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/bin/Debug/net8.0/Apsim.Workflow.dll",
            "preLaunchTask": "Build Debug",            
            "cwd": "${workspaceFolder}",            
            "console": "integratedTerminal",
            "justMyCode": false,
            "requireExactSource": false,
            "logging": {
                "moduleLoad": false,
                "threadExit": false
            },
            "symbolOptions": {
                "searchMicrosoftSymbolServer": false
            },
            "args": [
                "--sim-count",
                ""
            ]
        },
        {
            "name": "Apsim.Workflow: List directories",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/bin/Debug/net8.0/Apsim.Workflow.dll",
            "preLaunchTask": "Build Debug",            
            "cwd": "${workspaceFolder}",            
            "console": "integratedTerminal",
            "justMyCode": false,
            "requireExactSource": false,
            "logging": {
                "moduleLoad": false,
                "threadExit": false
            },
            "symbolOptions": {
                "searchMicrosoftSymbolServer": false
            },
            "args": [
                "-l",
                ""
            ]
        }
    ]
}
