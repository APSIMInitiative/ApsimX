                                  "Key": "AllowFertiliser",
                                  "Value": "no"
                                },
                                {
                                  "Key": "Depth2SoilN",
                                  "Value": "600"
                                },
                                {
                                  "Key": "CriticalSoilN",
                                  "Value": "100"
                                },
                                {
                                  "Key": "FertNAmount",
