                "        public string CropName { get; set; }",
                "",
                "        [Separator(\"Specify establishment and harvest conditions for this crop\")]",
                "",
                "        /// <summary>Date to establishment the crop</summary>",
                "        [Description(\"Establishment date (dd-mmm or dd/mm/yyyy):\")]",
