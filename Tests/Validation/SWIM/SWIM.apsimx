                        {
                          "$type": "Models.Manager, Models",
                          "Code": "using System;\r\nusing Models.Core;\r\nusing Models.Interfaces;\r\nusing Models.PMF;\r\nusing APSIM.Shared.Utilities;\r\nusing Models.Soils;\r\n\r\nnamespace Models\r\n{\r\n    [Serializable]\r\n    public class Script : Model\r\n    {\r\n        [Link] IClock Clock;\r\n        [Link] ISummary Summary;\r\n        [Link] Swim3 swim3;\r\n\r\n        [EventSubscribe(\"StartOfDay\")]\r\n        private void OnStartOfDay(object sender, EventArgs e)\r\n        {\r\n            if (!MathUtilities.FloatsAreEqual(swim3.PSI[0],0.0,0.1))\r\n                throw new Exception (\"Psi (\"+swim3.PSI[0].ToString()+\") is not equal to expected value of 0 in layer 0\");\r\n\r\n            if (!MathUtilities.FloatsAreEqual(swim3.PSI[1],-25.0,0.2))\r\n                throw new Exception (\"Psi (\"+swim3.PSI[1].ToString()+\") is not equal to expected value of -25 in layer 1\");\r\n\r\n            if (!MathUtilities.FloatsAreEqual(swim3.PSI[2],-100.0,2))\r\n                throw new Exception (\"Psi (\"+swim3.PSI[2].ToString()+\") is not equal to expected value of -100 in layer 2\");\r\n\r\n            if (!MathUtilities.FloatsAreEqual(swim3.PSI[3],-1000.0,10))\r\n                throw new Exception (\"Psi (\"+swim3.PSI[3].ToString()+\") is not equal to expected value of -1000 in layer 3\");\r\n\r\n            if (!MathUtilities.FloatsAreEqual(swim3.PSI[4],-15000.0,15))\r\n                throw new Exception (\"Psi (\"+swim3.PSI[4].ToString()+\") is not equal to expected value of -10000 in layer 4\");\r\n\r\n            if (!MathUtilities.FloatsAreEqual(swim3.PSI[5],-1000000.0,10000))\r\n                throw new Exception (\"Psi (\"+swim3.PSI[5].ToString()+\") is not equal to expected value of -100000 in layer 5\");\r\n\r\n        }\r\n    }\r\n}\r\n",
                          "Parameters": [],
                          "Name": "Manager",
