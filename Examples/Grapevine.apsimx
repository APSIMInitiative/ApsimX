            {
              "$type": "Models.Manager, Models",
              "CodeArray": [
                "using APSIM.Core;",
                "using Models.PMF.Organs;",
                "using Models.Zones;",
                "using APSIM.Shared.Utilities;",
                "using Models.Soils;",
                "using System.Collections.Generic;",
                "using Models.PMF;",
                "using Models.Core;",
                "using System;",
                "using Models.Climate;",
                "",
                "namespace Models",
                "{",
                "    [Serializable]",
                "    public class Script : Model, IStructureDependency",
                "    {",
                "        [Link] private Weather weather;",
                "        public IStructure Structure { private get; set; }",
                "        ",
                "        ",
                "        private RectangularZone AlleyZone {get; set; }",
                "        private RectangularZone RowZone {get; set;}",
                "        private RectangularZone ThisZone {get; set;}",
                "        private double AreaWidth {get; set;}",
                "",
                "        public double FRadInt {get; set;}",
                "        public double ET {get; set;}",
                "        public double ES {get; set;}",
                "        public double PET {get; set;}",
                "        public double LAI {get; set;}",
                "        public double Height {get; set;}",
                "        public double RootDepth {get; set;}",
                "        public double PlantWidth {get; set;}",
                "",
                "",
                "        ",
                "",
                "        [EventSubscribe(\"StartOfSimulation\")]",
                "        private void DoStartOfSimulation(object sender, EventArgs e)",
                "        {",
                "               ",
                "               ThisZone = this.Parent.Parent as RectangularZone;",
                "               RowZone =  Structure.Find<IModel>(\"Row\", relativeTo: (INodeModel)this.Parent.Parent.Parent) as RectangularZone;",
                "               AlleyZone = (RectangularZone) Structure.Find<IModel>(\"Alley\", relativeTo: (INodeModel)this.Parent.Parent.Parent) as RectangularZone;",
                "               AreaWidth = AlleyZone.Width + RowZone.Width;",
                "               PlantWidth = 0;",
                "        }",
                "        ",
                "        [EventSubscribe(\"DoReportCalculations\")]",
                "        private void OnDoReportCalculations(object sender, EventArgs e)",
                "        {",
                "            if (ThisZone.Name == \"Row\")",
                "                {",
                "                FRadInt = ((double)Structure.Get(\"Grapevine.Leaf.RadiationIntercepted\", relativeTo: RowZone) * RowZone.Width)/(weather.Radn * AreaWidth);",
                "                ET = (double)Structure.Get(\"Grapevine.Leaf.Transpiration\", relativeTo: RowZone) * RowZone.Width/AreaWidth;",
                "                LAI = (double)Structure.Get(\"Grapevine.Leaf.LAI\", relativeTo: RowZone);",
                "                //LAIPerVine =  LAI*RowZone.Width*1.8;",
                "                //TreeInitialisation.Script.VineDistance;",
                "                //(double)Structure.Get(\"Grapevine.Leaf.LAI\", relativeTo: RowZone);",
                "                Height = (double)Structure.Get(\"Grapevine.Leaf.Height\", relativeTo: RowZone);",
                "                RootDepth = (double)Structure.Get(\"Grapevine.Root.Depth\", relativeTo: RowZone);",
                "                PlantWidth = (double)Structure.Get(\"Grapevine.Leaf.Width\", relativeTo: RowZone);",
                "                }",
                "            else",
                "                {",
                "                FRadInt = ((double)Structure.Get(\"Slurp.Leaf.RadiationIntercepted\", relativeTo: AlleyZone) * ThisZone.Width)/ (weather.Radn * AreaWidth);",
                "                ET = (double)Structure.Get(\"Slurp.Leaf.Transpiration\", relativeTo: AlleyZone) * ThisZone.Width/AreaWidth;",
                "                LAI = (double)Structure.Get(\"Slurp.Leaf.LAI\", relativeTo: AlleyZone);",
                "                Height = (double)Structure.Get(\"Slurp.Leaf.Height\", relativeTo: AlleyZone);",
                "                RootDepth = (double)Structure.Get(\"Slurp.Root.Depth\", relativeTo: AlleyZone);",
                "                }",
                "",
                "            ES = (double)Structure.Get(\"Soil.SoilWater.Es\", relativeTo: ThisZone) * ThisZone.Width/AreaWidth;",
                "            double PETWidth = Math.Min(Math.Max(PlantWidth,ThisZone.Width),AreaWidth);",
                "            PET = (double)Structure.Get(\"Soil.SoilWater.Eo\", relativeTo: ThisZone) * PETWidth/AreaWidth;",
                "        }",
                "    }",
                "}"
              ],
