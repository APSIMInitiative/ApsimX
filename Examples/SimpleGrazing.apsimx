                },
                {
                  "Key": "triggerDeficit",
                  "Value": "90"
                },
                {
                  "Key": "targetDeficit",
                  "Value": "95"
                },
                {
                  "Key": "returndays",
                  "Value": "1"
                },
                {
                  "Key": "maximumAmount",
                  "Value": "300"
                },
                {
                  "Key": "depthPAWC",
                  "Value": "100"
                }
              ],
