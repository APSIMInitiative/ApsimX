                          "Code": "using Models.Climate;\r\nusing APSIM.Shared.Utilities;\r\nusing Models.Soils.Nutrients;\r\nusing Models.Soils;\r\nusing Models.PMF;\r\nusing Models.Core;\r\nusing System.Xml.Serialization;\r\nusing System;\r\nusing Models.AgPasture;\r\nusing Models.Interfaces;\r\nusing System.Linq;\r\nusing Models.Functions;\r\nusing System.Diagnostics;\r\n\r\nnamespace Models\r\n{\r\n    [Serializable] \r\n    [System.Xml.Serialization.XmlInclude(typeof(Model))]\r\n    public class Script : Model\r\n    {\r\n        [Link] Clock clock;\r\n        [Link] ISummary summary;\r\n        [Link] IWeather weather;\r\n        [Link] Irrigation irrigation;\r\n        [Link] private ISoilWater waterBalance;\r\n        [Link] private IPlant[] plants;\r\n        [Link(ByName=true)] PastureSpecies AGPRyegrass;\r\n        //[Link(ByName=true)] PastureSpecies AGPWhiteClover;\r\n        [Link] private Swim3 Swim;\r\n       // [Link(ByName=true)] private NutrientPool NO3; \r\n\r\n\r\n        //[Description(\"Winter white clover generic growth limiting factor; default = 1\")]\r\n        //public double WinterWhiteGlfGeneric { get; set; }\r\n\r\n\r\n\r\n        [EventSubscribe(\"DoDailyInitialisation\")]\r\n        private void DoDailyInitialisation(object sender, EventArgs e)\r\n        {\r\n            if (clock.Today == clock.StartDate) \r\n            {\r\n                //AGPWhiteClover.GlfGeneric = 0.5;\r\n                //AGPRyegrass.UseColdStressFactor = 0;\r\n                //AGPRyegrass.Root.KNO3 = 0.1;\r\n                //Swim.ernode = 0.001;\r\n                //Swim.ersoil = 0.001;\r\n                //Swim.slcerr = 0.001;\r\n                \r\n                //Debugger.Break();\r\n                var denitrificationModel = FindByPath(\"[Nutrient].Denitrification.Rate\").Value as CERESDenitrificationModel;\r\n                denitrificationModel.DenitrificationRateModifier = 0.0001; // default is 0.0006\r\n\r\n                var nitrifN2ORateConst = FindByPath(\"[Nutrient].Nitrification.NLoss\").Value as Constant;\r\n                nitrifN2ORateConst.FixedValue = 0.002; // default is 0.002\r\n\r\n                denitrificationModel.IsInertActive = false; // default is true\r\n\r\n            }\r\n            else\r\n            {\r\n            }\r\n\r\n        }\r\n\r\n        [EventSubscribe(\"DoReportCalculations\")]\r\n        private void DoReportCalculations(object sender, EventArgs e)\r\n        {\r\n        }\r\n\r\n    }\r\n}\r\n",
