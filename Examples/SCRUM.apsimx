                      "$type": "Models.Manager, Models",
                      "CodeArray": [
                        "// This script can be used to apply irrigation automatically, based soil water status",
                        "// Blame to RCichota, last updated in 18/Mar/2025",
                        "",
                        "using System;",
                        "using System.Xml.Serialization;",
