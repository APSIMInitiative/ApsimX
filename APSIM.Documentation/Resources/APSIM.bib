	pages = {297--305},
	file = {Full Text PDF:C\:\\Work\\Zotero\\Profiles\\sjdp2frt.default\\zotero\\storage\\EN83G9U3\\Ball-Coelho et al. - 1992 - Root dynamics in plant and ratoon crops of sugar c.pdf:application/pdf;Snapshot:C\:\\Work\\Zotero\\Profiles\\sjdp2frt.default\\zotero\\storage\\4ADWNSBR\\BF00010975.html:text/html}
}

