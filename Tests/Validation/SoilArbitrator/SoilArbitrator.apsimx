                              "Key": "Depth2SoilN",
                              "Value": "500"
                            },
                            {
                              "Key": "CriticalSoilN",
                              "Value": "50"
                            },
                            {
                              "Key": "FertNAmount",
                              "Value": "25"
                            },
                            {
                              "Key": "FertType",
                              "Value": "UreaN"
                            },
                            {
                              "Key": "FertApplicationDepth",
                              "Value": "1"
                            },
                            {
                              "Key": "FertMinimumDays",
                              "Value": "1"
                            }
                          ],
                          "Name": "AutomaticFertiliser",
                          "IncludeInDocumentation": true,
                          "Enabled": true,
